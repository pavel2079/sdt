# these classes are only used in auto-generated templates
plugin.tx_transfermietwagen._CSS_DEFAULT_STYLE (
    textarea.f3-form-error {
        background-color:#FF9F9F;
        border: 1px #FF0000 solid;
    }

    input.f3-form-error {
        background-color:#FF9F9F;
        border: 1px #FF0000 solid;
    }

    .tx-transfer-mietwagen table {
        border-collapse:separate;
        border-spacing:10px;
    }

    .tx-transfer-mietwagen table th {
        font-weight:bold;
    }

    .tx-transfer-mietwagen table td {
        vertical-align:top;
    }

    .typo3-messages .message-error {
        color:red;
    }

    .typo3-messages .message-ok {
        color:green;
    }
)
